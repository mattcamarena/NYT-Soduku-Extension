{
  "manifest_version": 3,
  "name": "NYT-Sudoku-Extension",
  "version": "1.0",
  "description": "Colors in all matching numbers on the sudoku board.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.nytimes.com/puzzles/sudoku/easy","https://www.nytimes.com/puzzles/sudoku/medium", "https://www.nytimes.com/puzzles/sudoku/hard"],
      "js": ["content.js"]
    }
  ]
}
